<h1>Sign Up!!</h1>
<form action="<%= users_url %>" method="POST">
  <%= auth_token %>
  
  <label for="username">Username: 
    <input id="username" type="text" name="user[user_name]" value="<%= @user.user_name %>">
  </label>
  <br>
  <label for="password">Password: 
    <input id="password" type="password" name="user[password]" value="">
  </label>
  <br>
  <input type="submit" value="Sign Up">
</form>